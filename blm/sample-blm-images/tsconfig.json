{
  "compilerOptions": {
    "target":"ES6",
    "module": "commonjs",
    "moduleResolution": "node",
    "sourceMap": false,
    "diagnostics": false,
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": "./"
  },
  "filesGlob": [
    "./src/**/*.ts",
    "./webapp/**/*.ts"
  ],
  "exclude": [
    "./webapp/node_modules"
  ]
}

